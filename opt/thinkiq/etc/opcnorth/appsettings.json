{
  "AppConfiguration": {
    "ReconnectPeriod": 10,
    "SubToHistRequestList": true,
    "BrowseTagChunkSize": 15000,
    "DataHandler": {
      "Assembly": "ThinkIQ.JsonDataHandler",
      "Class": "ThinkIQ.JsonDataHandler.JsonFileLoader",
      "Params": {
        "DataRootDir": "../../DataRoot",
        "TagBatchSize": 1000,
        "MaxBatchSize": 4194304,
        "MaxBatchFileCount": 1000,
        "VariableFormat": true
      }
    },

    "DataStorer": {
        "Assembly": "ThinkIQ.JsonDataHandler",
        "Class": "ThinkIQ.JsonDataHandler.JsonDataStorer",
        "Params": {
             "DataRootDir": "../../DataRoot"
        }
    }

  }
}
