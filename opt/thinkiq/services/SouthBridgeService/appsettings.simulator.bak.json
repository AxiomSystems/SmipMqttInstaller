    "TagChangeCheckIntervalInSeconds": 60,
    "AttributeChangeCheckIntervalInSeconds": 3600,
    "AttributeChangeCheckTrigger": 0,				// Change to any different number to trigger attribute change check
    "MinAcquisitionPeriodInSeconds": "10",
    "MaxAcquisitionPeriodInSeconds": "3600",
    "AcquisitionDelayInSeconds": "30",
	"HistSeizedThresholdInSeconds": "1800"			// Report no data from historian after this many seconds
	
      //Default for the following is NOW,NOW
